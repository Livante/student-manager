require(["core/event", "jquery"], function(Event, $) {

    function qf_errorHandler(element, _qfMsg, escapedName) {
        var event = $.Event(Event.Events.FORM_FIELD_VALIDATION);
        $(element).trigger(event, _qfMsg);
        if (event.isDefaultPrevented()) {
            return _qfMsg == '';
        } else {
            // Legacy mforms.
            var div = element.parentNode;

            if ((div == undefined) || (element.name == undefined)) {
                // No checking can be done for undefined elements so let server handle it.
                return true;
            }

            if (_qfMsg != '') {
                var errorSpan = document.getElementById('id_error_' + escapedName);
                if (!errorSpan) {
                    errorSpan = document.createElement("span");
                    errorSpan.id = 'id_error_' + escapedName;
                    errorSpan.className = "error";
                    element.parentNode.insertBefore(errorSpan, element.parentNode.firstChild);
                    document.getElementById(errorSpan.id).setAttribute('TabIndex', '0');
                    document.getElementById(errorSpan.id).focus();
                }

                while (errorSpan.firstChild) {
                    errorSpan.removeChild(errorSpan.firstChild);
                }

                errorSpan.appendChild(document.createTextNode(_qfMsg.substring(3)));

                if (div.className.substr(div.className.length - 6, 6) != " error"
                        && div.className != "error") {
                    div.className += " error";
                    linebreak = document.createElement("br");
                    linebreak.className = "error";
                    linebreak.id = 'id_error_break_' + escapedName;
                    errorSpan.parentNode.insertBefore(linebreak, errorSpan.nextSibling);
                }

                return false;
            } else {
                var errorSpan = document.getElementById('id_error_' + escapedName);
                if (errorSpan) {
                    errorSpan.parentNode.removeChild(errorSpan);
                }
                var linebreak = document.getElementById('id_error_break_' + escapedName);
                if (linebreak) {
                    linebreak.parentNode.removeChild(linebreak);
                }

                if (div.className.substr(div.className.length - 6, 6) == " error") {
                    div.className = div.className.substr(0, div.className.length - 6);
                } else if (div.className == "error") {
                    div.className = "";
                }

                return true;
            } // End if.
        } // End if.
    } // End function.
    
    function validate_mod_assign_confirm_submission_form_submissionstatement(element, escapedName) {
      if (undefined == element) {
         //required element was not found, then let form be submitted without client side validation
         return true;
      }
      var value = '';
      var errFlag = new Array();
      var _qfGroups = {};
      var _qfMsg = '';
      var frm = element.parentNode;
      if ((undefined != element.name) && (frm != undefined)) {
          while (frm && frm.nodeName.toUpperCase() != "FORM") {
            frm = frm.parentNode;
          }
          value = frm.elements['submissionstatement'].checked? '1': '';

  if (value == '' && !errFlag['submissionstatement']) {
    errFlag['submissionstatement'] = true;
    _qfMsg = _qfMsg + '\n - Obligatoriu';
  }

          return qf_errorHandler(element, _qfMsg, escapedName);
      } else {
        //element name should be defined else error msg will not be displayed.
        return true;
      }
    }

    document.getElementById('id_submissionstatement').addEventListener('blur', function(ev) {
        validate_mod_assign_confirm_submission_form_submissionstatement(ev.target, 'submissionstatement')
    });
    document.getElementById('id_submissionstatement').addEventListener('change', function(ev) {
        validate_mod_assign_confirm_submission_form_submissionstatement(ev.target, 'submissionstatement')
    });


    function validate_mod_assign_confirm_submission_form() {
      if (skipClientValidation) {
         return true;
      }
      var ret = true;

      var frm = document.getElementById('mform1')
      var first_focus = false;
    
      ret = validate_mod_assign_confirm_submission_form_submissionstatement(frm.elements['submissionstatement'], 'submissionstatement') && ret;
      if (!ret && !first_focus) {
        first_focus = true;
        Y.use('moodle-core-event', function() {
            Y.Global.fire(M.core.globalEvents.FORM_ERROR, {formid: 'mform1',
                                                           elementid: 'id_error_submissionstatement'});
            document.getElementById('id_error_submissionstatement').focus();
        });
      }
;
      return ret;
    }


    document.getElementById('mform1').addEventListener('submit', function(ev) {
        try {
            var myValidator = validate_mod_assign_confirm_submission_form;
        } catch(e) {
            return true;
        }
        if (typeof window.tinyMCE !== 'undefined') {
            window.tinyMCE.triggerSave();
        }
        if (!myValidator()) {
            ev.preventDefault();
        }
    });

});
;
require(["core/log"], function(amd) { amd.setConfig({"level":"warn"}); });;
require(["core/page_global"], function(amd) { amd.init(); });
});
//]]>
</script>
