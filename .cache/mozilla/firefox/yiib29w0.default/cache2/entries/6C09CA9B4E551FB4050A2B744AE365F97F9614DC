function createTabDetaliiBlaBlaCar() {
	var divParent = createEl("div", {
		'id': 'content_tab_detalii_blablacar',
		'class': 'blablacar'
	});
	$(divParent).append("Economise"+String.fromCharCode(351)+"te p"+String.fromCharCode(226)+"n"+String.fromCharCode(259)+" la <span class=\"economie_blablacar\" id=\"pret_blablacar\"></span>");
	$(divParent).append(
		$(createEl("table",{'class':"blablacar","cellspacing":0,"cellpadding":0})).append(
			$(createEl("tr")).append(
				$(createEl("td")).append("<span style=\"font-size: 9px; color: #000000;\">"+String.fromCharCode(206)+"n colaborare cu:</span><br /><a href=\"https://www.blablacar.ro/cum-functioneaza-ridesharing\" target=\"_blank\"><img src=\""+pathToImg+"blablacar-logo.png\" style=\"margin-top: 5px;\" width=77 height=13 title=\"Ce este "+String.fromCharCode(351)+"i cum func"+String.fromCharCode(355)+"ioneaza Blablacar\" /></a>"),
				$(createEl("td",{style:"text-align: right;"})).append(
					$(createEl("a",{
						href:"#",
						'id': 'link_blablacar',
						'target': '_blank',
						'onclick': 'clickBlaBlaCar(this);'
					})).append(
						createEl("img",{src:pathToImg+"blablacar-buton2.jpg",style:"margin-top: 5px;",width:130,height:26})
					)
				)
			)
		)
	);
	return divParent;
};

