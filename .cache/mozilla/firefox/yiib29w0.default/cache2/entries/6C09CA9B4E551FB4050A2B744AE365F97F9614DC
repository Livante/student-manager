	var table = createEl("table", {
		'style': 'width: 100%;',
		'id': 'detalii_linie',
		'cellpadding': 1,
		'cellspacing': 0
	});
	$(table).append(tr_tur_retur, tr_distanta, tr_timp, tr_cost, tr_dis, tr_radio);
	return table;
};

