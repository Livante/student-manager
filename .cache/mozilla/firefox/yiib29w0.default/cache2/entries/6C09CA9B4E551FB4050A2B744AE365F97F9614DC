function createDivSosire() {
	var divSosire = createEl("div", {
		'id': 'div_sosire',
		'title': pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['textCoordVarsInputs'][pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['textCoordVarsInputs'].length - 1] + pageVars['texte']['click_derulare_ruta']
	}, shorterString(pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['textCoordVarsInputs'][pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['textCoordVarsInputs'].length - 1], pageVars['generale']['limit_caractere_ruta']));
	return divSosire;
};

function createIconExplicatiiDis() {
	var icon = createEl("img", {
		'src': pathToImg + 'icon-help.png',
		'alt': 'DIS',
		'title': pageVars['texte']['detalii_dis'],
		'style': 'vertical-align: middle; margin-left: 5px; margin-top: -4px;'
	});
	return icon;
};

function createTableConsum() {
	var tr_de_la = createEl("tr",{class:"point"});
	var tr_pana_la = createEl("tr",{class:"point"});
	
	$(tr_de_la).append(createEl("td",{colspan:2}));
	$(tr_pana_la).append(createEl("td",{colspan:2}));
	
	var tr_viteza = createEl("tr");
	$(tr_viteza).append(createEl("td", {
		'style': 'width: 60px;'
	}, pageVars['texte']['string_viteza'] + ':'), createEl("td", {
		'id': 'td_viteza'
	}));
	var tr_timp = createEl("tr");
	$(tr_timp).append(createEl("td", {}, pageVars['texte']['string_timp'] + ':'), createEl("td", {
		'id': 'td_timp'
	}));
	var tr_consum = createEl("tr");
	$(tr_consum).append(createEl("td", {}, pageVars['texte']['string_consum'] + ':'), createEl("td", {
		'id': 'td_consum'
	}));
	var tr_combi = createEl("tr", {
		'id': 'tr_combi'
	});
	var td_combi = createEl("td", {
		'colspan': 2
	});
	var table_combi = createEl("table", {
		'style': 'width: 100%; border-bottom: 1px solid #DEDEDE; margin-top: 3px;',
		'cellspacing': 0,
		'cellpadding': 0
	});
	var tr_combi_1 = createEl("tr", {
		'style': 'background-color: #DEDEDE;'
	});
	$(tr_combi_1).append(createEl("td", {
		'style': 'width: 80px;'
	}, pageVars['texte']['string_consum_total']), createEl("td", {
		'style': 'border-left: 2px solid white; border-right: 2px solid white; padding-left: 10px;'
	}, pageVars['texte']['string_cost']), createEl("td", {
		'style': 'padding-left: 10px; width: 50px;'
	}, pageVars['texte']['string_dis']));
	$(tr_combi_1).children("td:nth-child(3)").append(createIconExplicatiiDis());
	var tr_combi_2 = createEl("tr");
	$(tr_combi_2).append(createEl("td", {
		'id': 'td_consum_total'
	}), createEl("td", {
		'style': 'font-weight: bold; color: #DD0000; padding-left: 10px;',
		'id': 'td_cost'
	}), createEl("td", {
		'id': 'td_dis',
		'style': 'padding-left: 10px;'
	}));
	$(table_combi).append(tr_combi_1, tr_combi_2);
	$(td_combi).append(table_combi);
	$(tr_combi).append(td_combi);
	var tr_valori_medii = createEl("tr");
	$(tr_valori_medii).append(createEl("td", {
		'colspan': 2,
		'style': 'text-align: right; font-style: italic; font-size: 80%;'
	}, pageVars['texte']['valori_medii_aproximative']));
	var table = createEl("table", {
		'style': 'width: 100%;',
		'id': 'consum',
		'cellpadding': 1,
		'cellspacing': 1
	});
	$(table).append(tr_de_la, tr_pana_la, tr_viteza, tr_timp, tr_consum, tr_combi, tr_valori_medii);
	return table;
};

function createTabTaxi(title, pret) {
	var aTaxi = createEl("a", {
		'id': 'tab_alternativ_taxi',
		'class': 'ruta_alternativa',
		'onmouseover': 'mouseOverTabTaxi(' + pret + ');',
		'onmouseout': 'mouseOutTabTaxi();',
		'title': title
	}, pageVars['texte']['string_taxi']);
	return aTaxi;
};

function createTabRutaAlternativa(nr) {
	var aRutaAlternativa = createEl("a", {
		'class': 'ruta_alternativa' + ((nr - 1 == pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['optiuni']['ruta_selected']) ? '_selected' : ''),
		'onmouseover': 'mouseOverTabRuta(this,' + nr + ');',
		'onmouseout': 'mouseOutTabRuta(this,' + nr + ');',
		'onclick': 'clickTabRutaServer(this,' + nr + ');',
		'title': calculateDistanta(pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['results'][nr - 1]['info']['distanta']) + ', ' + transformSecundeToText(pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['results'][nr - 1]['info']['durata'], 'simbol')
	}, pageVars['texte']['ruta'] + ' ' + nr);
	return aRutaAlternativa;
};

function createTabRuteAlternativeMobile(nr){
	if (nr>1) {
		for (i = 0; i < nr; i++) {
			$("div.tabs_mobile").append(createTabRutaAlternativaMobile(i + 1));
		};
	};
};

function createTabRutaAlternativaMobile(nr) {
	var divRutaAlternativa = createEl("div", {
		'class': 'tab tab_mobile ruta_mobile' + ((nr - 1 == pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['optiuni']['ruta_selected']) ? '_selected' : ''),
		'onclick': 'clickTabRutaServer(this,' + nr + ');'
	}, pageVars['texte']['ruta'] + ' ' + nr);
	return divRutaAlternativa;
};

function createTabRuteAlternative(nr) {
	var divRuteAlternative = createEl("div", {
		'class': 'rute_alternative'
	});
	if (nr > 1) {
		for (i = 0; i < nr; i++) {
			$(divRuteAlternative).append(createTabRutaAlternativa(i + 1));
		};
	};
	return divRuteAlternative;
};

function createContentInfoWindow(obj) {
	var divInfoWindow = createEl("div", {
		'id': 'div_info_window'
	});
	var divNr = createEl("div", {
		'class': 'nr'
	}, $(obj).children(".nr").html() + '.');
	/* var imgPegman = createEl("img", {
		'src': pathToImg + 'pegman-off.gif',
		'title': pageVars['texte']['street_view_off'],
		'id': 'pegman',
		'onclick': 'wT.clickPegmanInfoWindow();'
	}); */
	// $(divInfoWindow).append(divNr, imgPegman);
	$(divInfoWindow).append(divNr);
	$(divInfoWindow).append($(obj).children(".text").html());
	return divInfoWindow;
};

function createContentInfoWindowStradaObiectiv(content, contentComplet) {
	var divInfoWindowStradaObiectiv = createEl("div", {
		'id': 'div_info_window_strada_obiectiv'
	});
	/* var imgPegman = createEl("img", {
		'src': pathToImg + 'pegman-off.gif',
		'title': pageVars['texte']['street_view_off'],
		'id': 'pegman',
		'onclick': 'wT.clickPegmanInfoWindowStradaObiectiv();'
	}); */
	var aDeAici = createEl('a', {
		'class': 'dela_panala',
		'href': 'javascript:void(0);',
		'onclick': 'wT.clickInfoWindowDeLa(\'' + contentComplet + '\');'
	}, pageVars['texte']['de_aici']);
	var aPanaAici = createEl('a', {
		'class': 'dela_panala',
		'href': 'javascript:void(0);',
		'onclick': 'wT.clickInfoWindowPanaLa(\'' + contentComplet + '\');'
	}, pageVars['texte']['pana_aici']);
	//$(divInfoWindowStradaObiectiv).append(imgPegman);
	$(divInfoWindowStradaObiectiv).append(content, createEl("br"), createEl("br"), createEl('span', {
		'style': 'font-size: 11px;'
	}, pageVars['texte']['ruta'] + ' '), aDeAici, createEl('span', {
		'style': 'font-size: 11px;'
	}, '/'), aPanaAici);
	return divInfoWindowStradaObiectiv;
};

var bannerMapBlaBlaCar = function () {
	var bgState1 = "state1.2.jpg";
	var bgState2 = "state2.2.jpg";
	var bgState3 = "state3.2.jpg";
	
	var ruta = "Bucure"+String.fromCharCode(351)+"ti - Bra"+String.fromCharCode(351)+"ov";
	var totalSuma = 78;
	var url = "http://blbl.cr/rodis01";
	
	var a = createEl("a",{class:"map",href:url,target:"_blank",onclick:"clickBannerMapBla(this);"});
	var redSize = (totalSuma>=1000)?"mare":"mic";
	var sumaMica = Math.round(totalSuma/4);
	var timing = 3500;
	var bannerTimeout;
	
	var imgClose = createEl("img",{
		src: pathToImg+'inchide-avansat.png',
		class: "close_banner"
	});
	
	var elem = new Array();
	
	this.create = function () {
		
		elem[0] = new Array(
			createEl("img",{src:pathToImg+bgState1,class:"bg"}),
			createEl("span",{class:"titlu"},ruta),
			createEl("div",{class:"red "+redSize},totalSuma+" Lei")
		);
		elem[1] = new Array(
			createEl("img",{src:pathToImg+bgState2,class:"bg"}),
			createEl("span",{class:"titlu"},ruta),
			createEl("div",{class:"green mic pos1"},sumaMica+" Lei"),
			createEl("div",{class:"green mic pos2"},sumaMica+" Lei"),
			createEl("div",{class:"green mare pos3"},sumaMica+" Lei"),
			createEl("div",{class:"green mare pos4"},sumaMica+" Lei")
		);
		elem[2] = new Array(
			createEl("img",{src:pathToImg+bgState3,class:"bg"}),
			createEl("span",{class:"mesaj"},"Ofer"+String.fromCharCode(259)+" locurile libere "+String.fromCharCode(351)+"i "+String.fromCharCode(238)+"mparte costurile"),
			$(createEl("div",{class:"white"}," ")).append(
				createEl("img",{src:pathToImg+"logoBla.png",class:"logo"}),
				createEl("img",{src:pathToImg+"buton2.jpg",class:"buton"})
			)
		);
		
		//$("body").append(imgClose);
		
		return $(a).empty().append(
			elem[0][0],
			elem[0][1],
			elem[0][2],
			
			elem[1][0],
			elem[1][1],
			elem[1][2],
			elem[1][3],
			elem[1][4],
			elem[1][5],
			
			elem[2][0],
			elem[2][1],
			elem[2][2]
		);
	};
	this.remove = function () {
		clearTimeout(bannerTimeout);
		$(a).remove();
	};
	this.update = function (vals) {
		if (typeof vals !== 'undefined') {
			// aici updatam toate variabilele la valorile din vals
			ruta = vals['orase'][0]+" - "+vals['orase'][1];
			totalSuma = vals['suma'];
			//url = "https://www.blablacar.ro/ofera-locuri-noi?p=distanta&hide_map=1&from="+encodeURIComponent(vals['orase'][0])+"&to="+encodeURIComponent(vals['orase'][1])+"&waypoints="+((vals['waypoints'].length>0)?encodeURIComponent(vals['waypoints'].join("|")):"")+"&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_FUNN&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_FUNN";
			//url = "https://www.blablacar.ro/ridesharing-nou?p=distanta&hide_map=1&from="+encodeURIComponent(vals['orase'][0])+"&to="+encodeURIComponent(vals['orase'][1])+"&waypoints="+((vals['waypoints'].length>0)?encodeURIComponent(vals['waypoints'].join("|")):"")+"&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_CARS2&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_CARS2";
			//url = "https://www.blablacar.ro/inscriere-conducatori-auto?p=distanta&hide_map=1&from="+encodeURIComponent(vals['orase'][0])+"&to="+encodeURIComponent(vals['orase'][1])+"&waypoints="+((vals['waypoints'].length>0)?encodeURIComponent(vals['waypoints'].join("|")):"")+"&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_EZDR&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_EZDR";
			url = "https://www.blablacar.ro/lp/rodistanta?redirect_to=blablacar_trip_publication_step1&p=distanta&hide_map=1&from="+encodeURIComponent(vals['orase'][0])+"&to="+encodeURIComponent(vals['orase'][1])+"&waypoints="+((vals['waypoints'].length>0)?encodeURIComponent(vals['waypoints'].join("|")):"")+"&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_NEWFUNN&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_NEWFUNN";
			//url = "https://www.blablacar.ro/ofera-locuri-noi?p=distanta&hide_map=1&from="+encodeURIComponent(vals['orase'][0])+"&to="+encodeURIComponent(vals['orase'][1])+"&waypoints="+((vals['waypoints'].length>0)?encodeURIComponent(vals['waypoints'].join("|")):"")+"&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_FUNN&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_FUNN";
			redSize = (totalSuma>=1000)?"mare":"mic";
			sumaMica = Math.round(totalSuma/4);
		} else {
			// aici resetam toate variabilele la cele default
			ruta = "Bucure"+String.fromCharCode(351)+"ti - Bra"+String.fromCharCode(351)+"ov";
			totalSuma = 78;
			//url = "https://www.blablacar.ro/ofera-locuri-noi?p=distanta&hide_map=1&from=&to=&waypoints=&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_FUNN&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_FUNN";
			//url = "https://www.blablacar.ro/ridesharing-nou?p=distanta&hide_map=1&from=&to=&waypoints=&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_CARS2&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_CARS2";
			//url = "https://www.blablacar.ro/inscriere-conducatori-auto?p=distanta&hide_map=1&from=&to=&waypoints=&comuto_cmkt=RO_DISTANTA_DRVR_BIGB_EZDR&utm_source=DISTANTA&utm_medium=banner&utm_campaign=RO_DISTANTA_DRVR_BIGB_EZDR";
			url = "http://blbl.cr/rodis01";
			redSize = (totalSuma>=1000)?"mare":"mic";
			sumaMica = Math.round(totalSuma/4);
		};
		$(a).attr({href:url});
	};
	var clearElem = function () {
		var myTransition	=	($.browser.webkit)  ? '-webkit-transition' :
						($.browser.mozilla) ? '-moz-transition' : 
						($.browser.msie)    ? '-ms-transition' :
						($.browser.opera)   ? '-o-transition' : 'transition',
		myCSSObj		=	{opacity : 0};
		myCSSObj[myTransition]	=	'opacity 1s ease-in-out';
		
		for (var i=0; i<elem.length; i++) {
			for (var j=0; j<elem[i].length; j++) {
				$(elem[i][j]).css(myCSSObj);
				$(elem[i][j]).children().css(myCSSObj);
			};
		};
	};
	this.start = function () {
		state1();
	};
	var state1 = function () {
		var myTransition	=	($.browser.webkit)  ? '-webkit-transition' :
						($.browser.mozilla) ? '-moz-transition' : 
						($.browser.msie)    ? '-ms-transition' :
						($.browser.opera)   ? '-o-transition' : 'transition',
		myCSSObj		=	{opacity : 1};
		myCSSObj[myTransition]	=	'opacity 1s ease-in-out';
		
		clearElem();
		
		for (var i=0; i<elem[0].length; i++) {
			$(elem[0][i]).css(myCSSObj);
			$(elem[0][i]).children().css(myCSSObj);
		};
		bannerTimeout = setTimeout(state2, timing);
	};
	var state2 = function () {
		var myTransition	=	($.browser.webkit)  ? '-webkit-transition' :
						($.browser.mozilla) ? '-moz-transition' : 
						($.browser.msie)    ? '-ms-transition' :
						($.browser.opera)   ? '-o-transition' : 'transition',
		myCSSObj		=	{opacity : 1};
		myCSSObj[myTransition]	=	'opacity 1s ease-in-out';
		
		clearElem();
		
		for (var i=0; i<elem[1].length; i++) {
			$(elem[1][i]).css(myCSSObj);
			$(elem[1][i]).children().css(myCSSObj);
		};
		bannerTimeout = setTimeout(state3, timing);
	};
	var state3 = function () {
		var myTransition	=	($.browser.webkit)  ? '-webkit-transition' :
						($.browser.mozilla) ? '-moz-transition' : 
						($.browser.msie)    ? '-ms-transition' :
						($.browser.opera)   ? '-o-transition' : 'transition',
		myCSSObj		=	{opacity : 1};
		myCSSObj[myTransition]	=	'opacity 1s ease-in-out';
		
		clearElem();
		
		for (var i=0; i<elem[2].length; i++) {
			$(elem[2][i]).css(myCSSObj);
			$(elem[2][i]).children().css(myCSSObj);
		};
		bannerTimeout = setTimeout(state1, timing);
	};
};

var bannerMapUber = function () {
	
	var url = "https://get.uber.com/cl/ro/?utm_source=distanta&utm_campaign=distanta";
	
	var a = createEl("a",{class:"map_uber",href:url,target:"_blank",onclick:"clickBannerMapUber(this);"});
	
	this.create = function () {
		var banners = ["uber-1.png", "uber-2.png", "uber-3.png", "uber-4.png"];
		var imagine = banners[Math.floor(Math.random() * banners.length)];
		return $(a).empty().append(
			createEl("img",{src:pathToImg+imagine,class:"bg"})
		);
	};
	this.remove = function () {
		$(a).remove();
	};
};

