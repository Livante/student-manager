	}, shorterString(pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['results'][0]['nume_tip_mijloc'] + ' ' + pageVars[tipRuta]['orase'][idOrasSelected][moduriCautare]['results'][0]['nume'], pageVars['generale']['limit_caractere_ruta']));
