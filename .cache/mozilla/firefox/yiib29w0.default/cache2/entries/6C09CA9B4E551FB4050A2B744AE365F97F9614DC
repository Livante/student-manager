function createTabDetaliiAutohop() {
	var divParent = createEl("div", {
		'id': 'content_tab_detalii_autohop'
	});
	var divBgLateral = createEl("div", {
		'class': 'bg-content-tab'
	});
	var divBgJos = createEl("div", {
		'class': 'bg-jos-content-tab'
	});
	$(divParent).append(createTableAutohop(), divBgLateral, divBgJos);
	return divParent;
};

function createTableAutohop() {
	var table = $(createEl("table",{'class':"autohop","cellspacing":0,"cellpadding":0})).append(
		$(createEl("tr")).append(
			$(createEl("td",{"width":"48%","valign":"top","style":"font-size: 12px;"})).append("<b>Ofer"+String.fromCharCode(259)+"-"+String.fromCharCode(355)+"i locul liber!</b><br /><br /><span style=\"font-size: 9px; margin-left: 7px;\">"+String.fromCharCode(206)+"n colaborare cu:</span><br /><a href=\"https://www.autohop.ro/infopages/learnmore\" target=\"_blank\"><img src=\""+pathToImg+"autohop-logo.png\" title=\"Ce este "+String.fromCharCode(351)+"i cum func"+String.fromCharCode(355)+"ioneaz"+String.fromCharCode(259)+" AutoHop\" style=\"margin-top: 5px; margin-left: 7px;\" width=72 height=14 /></a>"),
			$(createEl("td",{"width":"52%","valign":"top","style":"text-align: center;color: #009A2C;"})).append("Economise"+String.fromCharCode(351)+"te p"+String.fromCharCode(226)+"n"+String.fromCharCode(259)+" la<br /><span class=\"economie_autohop\" id=\"pret_autohop\"></span><br /><a href=\"#\" id=\"link_autohop\" onclick=\"clickAutohop(this);\" target=\"_blank\"><img src=\""+pathToImg+"autohop-buton.jpg\" style=\"margin-top: 5px;\" width=105 height=25 /></a>")
		)
	);
	return table;
};

function createTabDetaliiConsum() {
	var divParent = createEl("div", {
		'id': 'content_tab_detalii_consum'
	});
	var divBgLateral = createEl("div", {
		'class': 'bg-content-tab'
	});
	var divBgJos = createEl("div", {
		'class': 'bg-jos-content-tab'
	});
	$(divParent).append(createTableConsum(), divBgLateral, divBgJos);
	return divParent;
};

function createPlusMinusConsum() {
	var divImgMinusPlus = createEl("div", {
		'id': 'div_img_minus_plus_consum',
		'style': 'float: right;'
	});
	var imgMinus = createEl("img", {
		'src': pathToImg + 'minus-mic.png',
		'style': 'cursor: pointer;',
		'onclick': 'modifyConsum(\'minus\');',
		'title': pageVars['texte']['micsorati_consumul']
	});
	var imgPlus = createEl("img", {
		'src': pathToImg + 'plus-mic.png',
		'style': 'cursor: pointer; margin-left: 7px;',
		'onclick': 'modifyConsum(\'plus\');',
		'title': pageVars['texte']['mariti_consumul']
	});
	$(divImgMinusPlus).append(imgMinus, imgPlus);
	return divImgMinusPlus;
};

